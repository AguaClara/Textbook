@article{casson_flocculation_1990,
	title = {Flocculation in {Turbulent} {Flow}: {Measurement} and {Modeling} of {Particle} {Size} {Distributions}},
	volume = {82},
	issn = {0003-150X},
	shorttitle = {Flocculation in {Turbulent} {Flow}},
	url = {http://www.jstor.org/stable/41293000},
	abstract = {Fluid motion in an oscillating-grid flocculator was investigated quantitatively with a laser Doppler velocimeter for two mixing conditions. Batch flocculation experiments were performed for monodisperse, bimodal, and trimodal latex suspensions. The effects of mixing intensity, particle charge, and suspension concentration on the rate of change in particle size distributions were studied. Experimental results indicated that large eddies are relatively unimportant for flocculation and that flocculation is induced by eddies of approximately the same size as the particles. A method for estimating the velocity gradients in eddies of different sizes in the flow was developed and incorporated into an existing mathematical model for flocculation. The predictions of the revised flocculation model were compared with the experimental results and indicated that the velocity gradients of the eddies adequately describe the interparticle contacts resulting from mixing during flocculation. El movimiento de fluidos en un floculador oscilante de reja fue investigado cuantitativamente con un velocímetro láser Doppler para dos tipos de mezcla. Se hicieron experimentos en conjuntos de floculación para suspensiones monodispersas, bimodales, y trimodales de tipo látex. Se estudiaron los efectos que tienen la intensidad del mezclado, la carga de las partículas, y la concentración de suspensión sobre el índice de cambios en la distribución según el tamaño de las partículas. Los resultados de los experimentos demostraron que remolinos grandes tienen poca importancia en la floculación, y que la floculación es inducida por remolinos de aproximadamente el mismo tamaño de las partículas. Se creó un método para calcular los gradientes de velocidad en remolinos de diferente tamaño de flujo y fue incorporado en un modelo matemático de floculación.},
	number = {8},
	urldate = {2015-11-23},
	journal = {Journal (American Water Works Association)},
	author = {Casson, Leonard W. and Lawler, Desmond F.},
	year = {1990},
	pages = {54--68},
	file = {Casson_Lawler_1990_Flocculation in Turbulent Flow.pdf:C\:\\Users\\William\\Documents\\Research\\zotero\\storage\\PSAQRC53\\Casson_Lawler_1990_Flocculation in Turbulent Flow.pdf:application/pdf}
}

